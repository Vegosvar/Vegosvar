<% if (typeof post.user_info.active_photo === 'undefined') { %>
	/assets/images/no-picture.svg
<% } else {
	if(post.user_info.active_photo === 'vegosvar') { %>
		<%= post.user_info.vegosvar_photo %>
	<% } else if (post.user_info.active_photo === 'facebook') { %>
		<%= post.user_info.fb_photo %>
	<% } else { %>
		/assets/images/no-picture.svg
	<% }
} %>